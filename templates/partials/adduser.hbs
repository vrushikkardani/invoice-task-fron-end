<!-- Page Content -->
<div class="content container-fluid">
	<div class="row">
		<div class="col-md-12">
			<div>
				<table class="table table-striped custom-table mb-0 datatable">
					<thead>
						<tr>
							<th>Email Id</th>
							
							<th class="text-right">Action</th>
						</tr>
					</thead>
					<tbody id="tbd">
						
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
<!-- Add Department Modal -->
<div id="add_department" class="modal custom-modal fade" role="dialog">
					<div class="modal-dialog modal-dialog-centered" role="document">
						<div class="modal-content">
							<div class="modal-header">
								<h5 class="modal-title">Add User</h5>
								<button type="button" class="close" data-dismiss="modal" aria-label="Close">
									<span aria-hidden="true">&times;</span>
								</button>
							</div>
							<div class="modal-body">
								<form>
									<div class="form-group">
										<label>Email Id <span class="text-danger">*</span></label>
										<input class="form-control" id="email" type="email" required>
									</div>
									<div class="form-group">
										<label>Password <span class="text-danger">*</span></label>
										<input class="form-control" id="password" type="password" required>
									</div>
									<p id="error" class="text-center text-danger"></p>
									<p id="ps" class="text-center text-danger"></p>
									<div class="submit-section">
										<button class="btn btn-primary submit-btn" onclick="adduser()">Submit</button>
									</div>
								</form>
							</div>
						</div>
					</div>
				</div>
<div id="edit_department" class="modal custom-modal fade" role="dialog">
					<div class="modal-dialog modal-dialog-centered" role="document">
						<div class="modal-content">
							<div class="modal-header">
								<h5 class="modal-title">Edit User Id</h5>
								<button type="button" class="close" data-dismiss="modal" aria-label="Close">
									<span aria-hidden="true">&times;</span>
								</button>
							</div>
							<div class="modal-body">
								<form>
									<div class="form-group">
										<label>Email Id <span class="text-danger">*</span></label>
										<input class="form-control" id="editemail" type="text">
										<input class="form-control" id="id1" type="text" hidden>
									</div>
									<p id="error1" class="text-center text-danger"></p>
									<div class="submit-section">
										<button class="btn btn-primary submit-btn" onclick="updateuser()">Submit</button>
									</div>
								</form>
							</div>
						</div>
					</div>
				</div>
<!-- /Add Department Modal -->

</div>
<!-- /Page Content -->