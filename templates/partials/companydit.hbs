<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title mb-0">Company Details</h4>
            </div>
            <div class="card-body">
                <form action="#">
                    <div class="row">
                        <div class="col-md-12">
                            <div>
                                <label>Company Logo</label>
                                <input type="file" id="image" name="filename" onchange="loadFile(event)" />
                                <p><img id="output" width="200"/></p><button onclick="remove()">Remove</button>
                            </div>
                            <br>
                            <div class="form-group">
                                <label>Company Name</label>
                                <input type="text" class="form-control" id="Company_Name">
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>Address</label>
                                <textarea rows="5" cols="5" class="form-control" style="height: 125px;"
                                    id="Address"></textarea>
                            </div>
                        </div>
                        <div class="col-md-6">

                            <div class="form-group">
                                <label>MobileNo.</label>
                                <input type="text" class="form-control" id="Mobile_No">
                            </div>
                            <div class="form-group">
                                <label>Email</label>
                                <input type="text" class="form-control" id="Email_Id">
                            </div>
                            <div class="form-group">
                                <label>PAN No.</label>
                                <input type="text" class="form-control" id="PAN_No">
                            </div>
                            <div class="form-group">
                                <label>BankName</label>
                                <input type="text" class="form-control" id="Bank_Name">
                            </div>
                            <div class="form-group">
                                <label>AccountNo</label>
                                <input type="text" class="form-control" id="Account_No">
                            </div>
                            <div class="form-group">
                                <label>ContactPerson</label>
                                <input type="text" class="form-control" id="ContactPerson">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label>PhoneNo.</label>
                                <input type="text" class="form-control" id="Phone_No">
                            </div>
                            <div class="form-group">
                                <label>GST No.</label>
                                <input type="text" class="form-control" id="GSTIN_No">
                            </div>
                            <div class="form-group">
                                <label>Website</label>
                                <input type="text" class="form-control" id="Website">
                            </div>
                            <div class="form-group">
                                <label>BranchName</label>
                                <input type="text" class="form-control" id="Branch_Name">
                            </div>
                            <div class="form-group">
                                <label>IFSC Code</label>
                                <input type="text" class="form-control" id="IFSC_Code">
                            </div>
                            <br>
                            <div class="form-group">
                                <label>Signature</label>
                                <input type="file" id="image2" name="filename" onchange="loadFilee(event)">
                                <p><img id="outputt" width="200"/></p><button onclick="remove1()">Remove</button>
                            </div>
                        </div>

                    </div>
                    <p id="error4" class="text-center text-danger"></p>

                    <div class="submit-section">
                        <button class="btn btn-primary submit-btn" onclick="addcompanydit()">Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>